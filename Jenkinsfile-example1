pipeline {
  agent any
  stages {
    stage('verify installation') {
      steps {
        sh 'bru --version'
      }
    }
    stage('run get') {
      steps {
        sh 'bru run example1/get.bru'
      }
    }
    stage('run example1 collection') {
      steps {
        sh 'bru run example1'
      }
    }
  }
}